/* pages/search/search.wxss */
page{
    background: #f5f5f5;
}
.course-content{
    width: 100%;
    display: flex;
    flex-direction: column;
}
v-videobox{
    margin-top: 15rpx;
}
.top-part{
    background-color: #ffffff;
    position:fixed;
    height: 100rpx;
    border-top: 1rpx solid #f5f5f5;
    border-bottom: 1rpx solid #f5f5f5;
    display: flex;
    flex-direction: row;
    width: 100%;
    align-items: center;
    z-index:99;
}
.search-bar{
    background-color: #f5f5f5;
}
.top-part .cancel {
    line-height: 82rpx;
    width: 100rpx;
    text-align: center;
    display: inline-block;
    color: #666;
    font-size: 28rpx;
}